{
  "welcomeFile": "/test/flpSandbox.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/trackpurchaseorders/(.*)$",
      "target": "$1",
      "destination": "lbn-gtt-pof-api"
    },
    {
      "source": "^/(token.json)(.*)$",
      "target": "$1",
      "localDir": "resources"
    },
    {
      "source": "^(.*)$",
      "localDir": "../webapp",
      "cacheControl": "no-cache"
    }
  ]
}
